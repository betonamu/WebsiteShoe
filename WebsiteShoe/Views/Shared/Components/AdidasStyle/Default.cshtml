@model List<WebsiteShoe.Models.ShoeStyleHeader>
<div class="mega-column">
    <h4 class="mega-heading">Adidas</h4>
    <ul class="mega-item">
        <li><a href="/Product/Adidas">Tất cả</a></li>
        @foreach (var item in Model)
        {
            <li><a href="/Product/Adidas/@item.StyleName">@item.StyleName</a></li>
        }
    </ul>
</div>
